<?xml version="1.0" encoding="utf-8" ?>
<log4net>
  <appender name="DefaultAppender" type="log4net.Appender.RollingFileAppender">
    <file value="D:\Desktop\ALPED\Systex\C#_VS\"/>　 <!--檔案存放路徑-->
    <staticLogFileName value="false"/>
    <appendToFile value="true"/>
    <rollingStyle value="Date"/>
    <datePattern value="yyyy-MM-dd.lo\g"/>　<!--log檔的命名 ex TRACE-01.lo\g -->
    <layout type="log4net.Layout.PatternLayout">
      <!--內容格式-->
      <conversionPattern value="%-5p %date{yyyy/MM/dd HH:mm:ss} %-20c{1} %-20M %m%n" />
    </layout>
  </appender>

  <appender name="WebAppender" type="log4net.Appender.RollingFileAppender">
    <file value="D:\Desktop\ALPED\Systex\C#_VS\"/>
    <!--檔案存放路徑-->
    <staticLogFileName value="false"/>
    <appendToFile value="true"/>
    <rollingStyle value="Composite"/>
    <maxSizeRollBackups value="-1"/>
    <!--maxSizeRollBackups設為-1，才不會限制總檔案數量(若設為10則僅會保留最新的前10個log檔)-->
    <maximumFileSize value="5KB"/>
    <datePattern value="yyyy-MM-dd.lo\g"/>
    <!--log檔的命名 ex TRACE-01.log -->
    <layout type="log4net.Layout.PatternLayout">
      <!--內容格式-->
      <conversionPattern value ="%date{yyyy/MM/dd} | %date{HH:mm:ss,fff} | %-5p | %-20m | %-20class | %-20M | %date{HH:mm:ss,fff}[%t]+%r{HH:mm:ss,fff} |%n"/>
    </layout>
  </appender>

  <appender name="InfoAppender" type="log4net.Appender.RollingFileAppender">
    <file value="D:\Desktop\ALPED\Systex\C#_VS\Log檔\"/>
    <!--檔案存放路徑-->
    <!--记录日志写入文件时，不锁定文本文件，防止多线程时不能写Log，官方说线程非安全-->
    <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    <staticLogFileName value="false"/>
    <appendToFile value="true"/>
    <rollingStyle value="Composite"/>
    <maxSizeRollBackups value="-1"/>
    <!--maxSizeRollBackups設為-1，才不會限制總檔案數量(若設為10則僅會保留最新的前10個log檔)-->
    <maximumFileSize value="256MB"/>
    <datePattern value="yyyy-MM-dd/&quot;TRACE-01.log&quot;"/>
    <!--log檔的命名 ex TRACE-01.log -->
    <layout type="Transfer_File.Log4net_Converter_Layout.PatternLayout">
      <!--內容格式-->
      <conversionPattern value ="%date{yyyy-MM-dd} | %date{HH:mm:ss.fff} | %-5p | %-20Property{Key} | %-30Property{ServiceName} | %-30Property{FunctionName} | %-15Property{ExecuteStartTime} | %-15Property{ExecuteEndTime} | %Property{Message}%n"/>
    </layout>
  </appender>

  <appender name="ErrorAppender" type="log4net.Appender.RollingFileAppender">
    <file value="D:\Desktop\ALPED\Systex\C#_VS\Log檔\"/>
    <!--檔案存放路徑-->
    <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    <staticLogFileName value="false"/>
    <appendToFile value="true"/>
    <rollingStyle value="Composite"/>
    <maxSizeRollBackups value="-1"/>
    <!--maxSizeRollBackups設為-1，才不會限制總檔案數量(若設為10則僅會保留最新的前10個log檔)-->
    <maximumFileSize value="256MB"/>
    <datePattern value="yyyy-MM-dd/&quot;TRACE-01.log&quot;"/>
    <!--log檔的命名 ex TRACE-01.log -->
    <layout type="Transfer_File.Log4net_Converter_Layout.PatternLayout">
      <!--內容格式-->
      <conversionPattern value ="%date{yyyy-MM-dd} | %date{HH:mm:ss.fff} | %-5p | %-20Property{Key} | %-30Property{ServiceName} | %-30Property{FunctionName} | %-15Property{ExecuteStartTime} | %-15Property{ExecuteEndTime} | %Property{Message}%n"/>
    </layout>
  </appender>

  <appender name="DebugAppender" type="log4net.Appender.RollingFileAppender">
    <file value="D:\Desktop\ALPED\Systex\C#_VS\Log檔\"/>
    <!--檔案存放路徑-->
    <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    <staticLogFileName value="false"/>
    <appendToFile value="true"/>
    <rollingStyle value="Composite"/>
    <maxSizeRollBackups value="-1"/>
    <!--maxSizeRollBackups設為-1，才不會限制總檔案數量(若設為10則僅會保留最新的前10個log檔)-->
    <maximumFileSize value="256MB"/>
    <datePattern value="yyyy-MM-dd/&quot;TRACE-01.log&quot;"/>
    <!--log檔的命名 ex TRACE-01.log -->
    <layout type="Transfer_File.Log4net_Converter_Layout.PatternLayout">
      <!--內容格式-->
      <conversionPattern value ="%date{yyyy-MM-dd} | %date{HH:mm:ss.fff} | %-5p | %-20Property{Key} | %-30Property{ServiceName} | %-30Property{FunctionName} | %-15Property{ExecuteStartTime} | %-15Property{ExecuteEndTime} | %Property{Message}%n"/>
    </layout>
  </appender>
  <root>
    <!--root的設定可做為未指定logger的預設值來使用-->
    <level value="ALL"/>
    <append-ref ref="DefaultAppender"/>
  </root>
  <!--<logger name ="Web">
        <level value="INFO" />
        <appender-ref ref="WebAppender"/>
        -->
  <!--指定了寫出log的詳細設定要ref到的Appender，在此我們定義Web的相關設定為WebAppender-->
  <!--
      </logger>-->
  <logger name ="InfoLogger">
    <level value="INFO" />
    <appender-ref ref="InfoAppender"/>
  </logger>
  <logger name ="ErrorLogger">
    <level value="WARN"/>
    <appender-ref ref="ErrorAppender"/>
  </logger>
  <logger name ="DebugLogger">
    <level value="DEBUG"/>
    <appender-ref ref="DebugAppender"/>
  </logger>
</log4net>